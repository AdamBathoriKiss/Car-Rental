<div class="container">
    <div class="row">
     

  {{#each cars}}
   <div class="card col-md-4">
  <img src="{{imageUrl}}" class="card-img-top" alt="Honda Civic">
  <div class="card-body">
    <h5 class="card-title">{{this.make}} {{this.model}}</h5>
    <p class="card-text">{{this.description}}</p>
  </div>
    <ul>
    <li class="list-group-item">{{this.make}} </li>
    <li class="list-group-item">{{this.model}}</li>
    <li class="list-group-item">{{this.yearOfProd}}</li>
    <li class="list-group-item">{{this.engine}}</li>
    <li class="list-group-item">{{this.transmission}}</li>
    <li class="list-group-item">{{this.fuel}}</li>
    <li class="list-group-item">{{this.extras}} </li>
    <li class="list-group-item">{{this.rentalCost}}</li>
  </ul> 
    <form action="/cars/{{_id}}/delete" method="POST"> 
    <a class="btn btn-warning" href="/cars/{{_id}}/edit">Edit</a>
    <button class="btn btn-danger">Delete</button>
    
        </form>
    </div>
  {{/each}}

</div>
    </div>
    













